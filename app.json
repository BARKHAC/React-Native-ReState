{
  "expo": {
    "name": "React-Native-ReState",
    "slug": "React-Native-ReState",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/cashbackedlogo.png",
    "scheme": "mystationery",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barkhac.ReactNativeReState",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/cashbackedlogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.barkhac.ReactNativeReState",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/cashbackedlogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/cashbackedlogo.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Montserrat-Italic-VariableFont_wght.ttf",
            "./assets/fonts/Montserrat.ttf",
            "./assets/fonts/Montserrat-Bold.ttf",
            "./assets/fonts/Montserrat-ExtraBold.ttf",
            "./assets/fonts/Montserrat-SemiBold.ttf",
            "./assets/fonts/Montserrat-Regular.ttf",
            "./assets/fonts/Montserrat-Medium.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "501ea36d-67ba-4a35-98f1-9afc8c00bc86"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/501ea36d-67ba-4a35-98f1-9afc8c00bc86"
    }
  }
}
